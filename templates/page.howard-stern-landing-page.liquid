          {% for product in collections["best-sellers-howard-stern"].products %}
          

              {%- assign iteration_number = forloop.index -%}
              {% assign current_variant = product.selected_or_first_available_variant %}

              {%unless iteration_number > 3 %}
              
                  <div class="shop-flex-col-item center">
                    <a href="{{ product.url }}" class="shop-img-wrapper w-inline-block">
                      <img src="{{ product.featured_image | product_img_url: 'medium' }}" alt="{{ product.title | escape }}" loading="lazy"  sizes="(max-width: 479px) 89.765625px, (max-width: 767px) 160px, 200px" class="img-shop"></a>
                    <div id="w-node-_2c453dd0-c9ca-8595-de3c-3a5b7b5c5cc5-dd266cfc" class="shop-name-inner">
                      <h2 class="black-text no-margin-b no-margin-t">
                        <a href="{{ product.url }}" class="black-bright-blue-link">{{product.title}}</a><span class="break-line"></span>
                      </h2>

                    {% if product.metafields.product_info.collection_description %}
                      <p class="black-text smaller">{{ product.metafields.product_info.collection_description }}</p>
                    {% endif %}
                        
                    </div>

                    <p class="black-text center">{{product.price | money }}</p>

                  </div>

                  {%endunless%}

                  

          {% endfor %}
        </div>
      </div>


      <a href="https://funkychunky.com/collections/best-selling-products" class="general-btn dark-with-w-text margin-t-larger w-button">Shop All Best Sellers</a>
